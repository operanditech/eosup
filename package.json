{
  "name": "eosup",
  "version": "1.4.0",
  "description": "A cross-platform local testnet manager and smart contract compiler for EOSIO",
  "keywords": [
    "eosio",
    "eos",
    "testnet",
    "compiler",
    "smart-contract",
    "seeds"
  ],
  "scripts": {
    "clean:deps": "rimraf node_modules",
    "clean:dist": "rimraf dist",
    "clean": "npm run clean:dist && npm run clean:deps",
    "build": "npm run clean:dist && tsc -p tsconfig.build.json",
    "watch": "npm run clean:dist && tsc -p tsconfig.build.json --watch",
    "lint": "tslint --project tsconfig.json --config tslint.json \"src/**/*.ts\" \"test/**/*.ts\"",
    "lint-fix": "npm run lint -- --fix",
    "test": "mocha -r ts-node/register \"test/*.spec.ts\" --timeout 30000",
    "prepare": "npm run build"
  },
  "author": "Andres Berrios <andres.berrios.j@gmail.com>",
  "repository": {
    "type": "git",
    "url": "https://github.com/operanditech/eosup.git"
  },
  "license": "MIT",
  "files": [
    "dist",
    "bin",
    "image",
    "systemContracts"
  ],
  "main": "dist/index.js",
  "bin": {
    "eosup": "./bin/eosup.js"
  },
  "dependencies": {
    "caporal": "^1.3.0",
    "dockerator": "0.3.6",
    "dockerode": "^2.5.8",
    "eosjs": "^20.0.0",
    "event-stream": "^4.0.1",
    "execa": "^2.0.3",
    "morpheos": "^2.3.0",
    "node-fetch": "^2.6.0"
  },
  "devDependencies": {
    "@types/dockerode": "^2.5.20",
    "@types/event-stream": "^3.3.34",
    "@types/execa": "^2.0.0",
    "@types/mocha": "^5.2.7",
    "mocha": "^10.2.0",
    "prettier": "^1.18.2",
    "rimraf": "^2.6.3",
    "ts-node": "^8.3.0",
    "tslint": "^5.18.0",
    "tslint-config-prettier": "^1.18.0",
    "tslint-plugin-prettier": "^2.0.1",
    "typescript": "^3.5.3"
  }
}
